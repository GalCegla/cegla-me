{
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "check-formatting": "prettier --check .",
    "check-typings": "tsc",
    "format": "prettier --write .",
    "db:start": "docker-compose up -d db",
    "db:stop": "docker-compose stop db",
    "db:dev": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:seed": "ts-node prisma/seed.ts",
    "prepare": "npm run generate",
    "rm:generated": "npx rimraf ./src/__generated__",
    "codegen": "apollo client:codegen --outputFlat --target typescript --addTypename src/__generated__",
    "generate": "npm run-s generate:db generate:graphql-server generate:graphql-client",
    "generate:db": "npx prisma generate",
    "generate:graphql-server": "npx cross-env NEXUS_SHOULD_GENERATE_ARTIFACTS=true NODE_PATH=./src npx ts-node --transpile-only src/schema",
    "generate:graphql-client": "npm run-s rm:generated codegen",
    "studio": "prisma studio"
  },
  "license": "0BSD",
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.11.3",
    "@prisma/client": "^2.23.0",
    "@types/react-helmet": "^6.1.1",
    "@types/react-lottie": "^1.2.5",
    "apollo": "^2.33.6",
    "apollo-upload-client": "^16.0.0",
    "eslint-plugin-graphql": "^4.0.0",
    "graphql": "^14.7.0",
    "howler": "^2.2.1",
    "lottie-web": "^5.7.11",
    "material-ui-color": "^1.2.0",
    "next": "^11.1.2",
    "nexus": "^1.1.0",
    "nexus-plugin-prisma": "^0.35.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-lottie": "^1.2.3",
    "react-lottie-segments": "^1.4.9",
    "run-s": "^0.0.0"
  },
  "devDependencies": {
    "@types/apollo-upload-client": "^14.1.0",
    "@types/howler": "^2.2.2",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^6.2.0",
    "prisma": "^2.23.0",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^9.1.0",
    "typescript": "^4.2.4"
  }
}
